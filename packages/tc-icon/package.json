  "keywords": [
    "trend-components",
    "react",
    "reactjs",
    "aria",
    "accessibility",
    "icons"
  ],
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trend-community/trend-components.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trend-community/trend-components/issues"
  },
  "homepage": "https://github.com/trend-community/trend-components#readme",
  "devDependencies": {
    "@svgr/cli": "^4.0.0",
    "jest": "^23.6.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "jest": {
    "setupFiles": [
      "../../setUpTests.js"
    ],
    "testRegex": ".+\\.spec\\.js$"
  }
}
