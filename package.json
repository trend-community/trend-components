{
  "devDependencies": {
    "@commitlint/cli": "7.0.0",
    "@commitlint/config-conventional": "7.0.1",
    "@commitlint/config-lerna-scopes": "7.0.0",
    "autoprefixer": "8.3.0",
    "babel-core": "6.26.0",
    "babel-jest": "23.4.0",
    "babel-loader": "7.1.4",
    "babel-plugin-prismjs": "1.0.2",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-traverse": "6.26.0",
    "babel-types": "6.26.0",
    "clean-webpack-plugin": "0.1.19",
    "commitizen": "2.10.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "cssom": "0.3.2",
    "cz-conventional-changelog": "2.1.0",
    "del-cli": "1.1.0",
    "enzyme": "3.3.0",
    "eslint": "4.19.1",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.0.0-rc.8",
    "jest": "23.4.1",
    "jstransformer-markdown": "1.2.1",
    "lerna": "2.10.2",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.8.3",
    "npm-run-all": "4.1.3",
    "postcss-loader": "2.1.4",
    "prismjs": "1.15.0",
    "prop-types": "15.6.2",
    "pug": "2.0.3",
    "pug-loader": "2.4.0",
    "raw-loader": "0.5.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-markdown": "3.3.4",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.4.1",
    "sass-loader": "7.0.1",
    "scss-parser": "1.0.0",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "0.8.1",
    "stylelint-scss": "3.0.1",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.4",
    "webpack-dev-server": "3.1.3"
  },
  "version": "0.1.0",
  "description": "Trend components UI component library.",
  "main": "none",
  "private": true,
  "scripts": {
    "build": "npm run clean && mkdir build && webpack --progress --colors",
    "build:docs": "npm run clean:docs && mkdir docs",
    "build:min": "mkdirp build && cross-env TC_ENV=production webpack -p --progress --colors",
    "clean": "npm-run-all --parallel clean:*",
    "clean:build": "del-cli build/** build",
    "clean:docs": "del-cli docs/** docs",
    "dev": "npm run clean && cross-env TC_ENV=development webpack-dev-server --config=webpack.demo.config.js --progress --inline",
    "dist": "npm-run-all --parallel build:*",
    "lint:css": "stylelint \"packages/**/*.scss\"",
    "start": "cross-env TC_ENV=development webpack-dev-server --config=webpack.dev.config.js --progress --inline",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional",
      "@commitlint/config-lerna-scopes"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "button",
          "helpers",
          "normalize",
          "theme",
          "type",
          "package",
          "demos",
          "scripts",
          "src",
          "root"
        ]
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
